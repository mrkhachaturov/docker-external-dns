{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "reporters": [
    [
      "jest-junit",
      {
        "suiteName": "docker-compose-external-dns e2e tests",
        "outputDirectory": "reports",
        "outputName": "test-results-e2e-junit.xml"
      }
    ]
  ]
}
